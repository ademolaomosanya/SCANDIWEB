{
    "version":2,
    "build":[
        {
            "src":"/dist/main.js",
            "use":"@vercel/node",
            "config":{
                "includefiles":[
                    "/dist/**"
                ]
            }
        }
        
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/dist/main.js"
        }
    ]
}