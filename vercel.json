{
    "version":2,
    "build":[
        {
            "src":"public/dist/main.js",
            "use":"@vercel/node",
            "config":{
                "includefiles":[
                    "public/dist/**"
                ]
            }
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"public/dist/main.js"
        }
    ]
}