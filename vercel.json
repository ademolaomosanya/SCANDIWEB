{
    "version":2,
    "builds":[
        {
            "src":"/src/main.ts",
            "use":"@vercel/node",
            "config":{
                "includefiles":[
                    "/dist/**"
                ]
            }
        }
        
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/src/main.ts"
        }
    ]
}